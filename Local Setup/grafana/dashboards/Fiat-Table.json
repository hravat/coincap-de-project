          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH ranked_data AS (\n    SELECT \n        dcr.*,\n        fcr.*,\n        ROW_NUMBER() OVER (\n            PARTITION BY dcr.currency_sr_key \n            ORDER BY fcr.event_timestamp DESC\n        ) AS rn\n    FROM coincap_prod.dim_currency_rate dcr   \n    INNER JOIN coincap_prod.fact_currency_rate fcr \n        ON dcr.currency_sr_key = fcr.currency_sr_key \n    WHERE dcr.currency_type = 'fiat'\n)\nSELECT UPPER(currency_id ) as \"Currency ID\",\n\t   case \n\t   \twhen currenncy_rate_in_usd = 0 then NULL\n\t   \telse 1/currenncy_rate_in_usd\n\t   end  \"Currency Rate\",\n       \t   case \n\t   \twhen currenncy_rate_in_usd = 0 then NULL\n\t   \telse 1/currenncy_rate_in_usd\n\t   end  \"Currency Rate Gauge\"\nFROM ranked_data\nWHERE rn = 1\norder by currenncy_rate_in_usd NULLS FIRST;\n",
